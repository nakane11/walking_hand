(defun walk
  (step)
  (let (angle-list)
    (dotimes (i step)
      (setq angle-list (append angle-list walk-angle)))
    (send *ri* :angle-vector-sequence angle-list (make-list (length angle-list) :initial-element 600))
    (send *ri* :wait-interpolation)
    ))

